{
    "version": "0.0.1",
    "name": "angular-schematics",
    "displayName": "Angular Schematics",
    "description": "Angular schematics commands from files Explorer or Command Palette.",
    "keywords": [
        "angular",
        "schematics",
        "angular schematics"
    ],
    "categories": [
        "Other"
    ],
    "icon": "icon-angular.png",
    "publisher": "cyrilletuzi",
    "author": {
        "name": "cyrilletuzi",
        "url": "https://www.cyrilletuzi.com/en/web/"
    },
    "contributors": [
        {
            "name": "cyrilletuzi",
            "url": "https://www.cyrilletuzi.com/en/web/"
        }
    ],
    "homepage": "https://github.com/cyrilletuzi/vscode-angular-schematics/",
    "repository": {
        "type": "git",
        "url": "https://github.com/cyrilletuzi/vscode-angular-schematics/"
    },
    "bugs": {
        "url": "https://github.com/cyrilletuzi/vscode-angular-schematics/issues"
    },
    "qna": false,
    "engines": {
        "vscode": "^1.24.0"
    },
    "activationEvents": [
        "workspaceContains:**/angular.json"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.generateComponent",
                "title": "Angular: Generate a component"
            },
            {
                "command": "extension.generateService",
                "title": "Angular: Generate a service"
            }
        ],
        "menus": {
            "explorer/context": [
                {
                    "command": "extension.generateComponent",
                    "group": "angularschematics@1"
                },
                {
                    "command": "extension.generateService",
                    "group": "angularschematics@2"
                }
            ]
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "tslint": "^5.8.0",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}
